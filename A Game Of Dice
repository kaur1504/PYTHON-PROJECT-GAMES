import random

l=[1,2,3,4,5,6]
ans=random.choice(l)

if ans==1:
    print(' __\n|     |\n|  o  |\n|     |\n|__|')
elif ans==2:
    print(' __\n|     |\n| o o |\n|     |\n|__|')
elif ans==3:
    print(' __\n|     |\n| o o |\n|  o  |\n|__|')
elif ans==4:
    print(' __\n|     |\n| o o |\n| o o |\n|__|')
elif ans==5:
    print(' __\n| o o |\n|  o  |\n| o o |\n|__|')
elif ans==6:
    print(' __\n| o o |\n| o o |\n| o o |\n|__|')
